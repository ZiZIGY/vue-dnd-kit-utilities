{
  "name": "@vue-dnd-kit/utilities",
  "version": "0.0.4",
  "description": "Utilities for Vue DnD Kit - a lightweight Vue 3 library for building performant and accessible drag and drop interfaces",
  "author": "ZiZIGY",
  "license": "MIT",
  "keywords": [
    "vue",
    "dnd",
    "drag",
    "drop",
    "drag and drop",
    "drag and drop library",
    "drag and drop component",
    "vue dnd",
    "vue drag and drop",
    "vue drag and drop library",
    "vue drag and drop component",
    "vue dnd kit",
    "vue dnd kit utilities",
    "dnd-kit",
    "dnd-kit utilities"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zizigy/vue-dnd-kit.git",
    "directory": "packages/core"
  },
  "scripts": {
    "start": "vite build --watch",
    "build": "vite build",
    "test": "vitest run",
    "lint": "eslint src --ext .ts,.vue",
    "prepublish": "npm run build"
  },
  "main": "dist/vue-dnd-kit-utilities.cjs.js",
  "module": "dist/vue-dnd-kit-utilities.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-dnd-kit-utilities.es.js",
      "require": "./dist/vue-dnd-kit-utilities.cjs.js"
    }
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "dist"
  ],
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.38.0",
    "typescript": "^4.9.5",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.5.0",
    "vitepress": "^1.6.3",
    "vue": "^3.5.13"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@vue-dnd-kit/core": "^0.0.21"
  }
}
